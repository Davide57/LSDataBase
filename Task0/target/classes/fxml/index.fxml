<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
id="rootPane"
styleClass="main-panel" 
xmlns:fx="http://javafx.com/fxml"
fx:controller="workinggroup.task0.Task0Controller">
    
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
   
    <Label styleClass="label" text="Your friendly bookstore is here"                
        GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>
    
    <TableView fx:id="Results" 
    GridPane.columnIndex="0" 
    GridPane.rowIndex="1"
    styleClass="table">
   
    </TableView>
    
    <Button text="Update" onAction="#submit_Button" styleClass="button">
    </Button>
    

</GridPane>   
    
    
    
 